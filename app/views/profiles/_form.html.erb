<%= form_with model: user, url: user_profile_path(user) do |user| %>
	<%= user.fields_for :profile_attributes, user.object.profile do |profile| %>
		<%= profile.label :bio %>
		<%= profile.text_area :bio %>

		<%= profile.label :likes %>
		<%= profile.text_area :likes %>

		<%= profile.label :dislikes %>
		<%= profile.text_area :dislikes %>

		<%= profile.label :gender %>
		<%= profile.text_field :gender %>

		<%= profile.label :sex_pref, 'What is your sexual preference' %>
		<%= profile.text_field :sex_pref %>
	<% end %>
	<%= user.submit 'Submit Public Profile' %>
<% end %>